SCRIPT FOR PARALLEL BROWSER

public class CrossBrowserScript {
   WebDriver driver;

   /**
    * This function will execute before each test tag i.e @Test in the
    * testing.xml
    * @param browser
    * @throws Exception
    */

   @BeforeTest
   @Parameters("browser")

   public void setup(String browser) throws Exception{
       //Check if the parameter passed from TestNG is 'Firefox'
       if (browser.equalsIgnoreCase(("firefox"))) {
           // create firefox instance
               System.setProperty("webdriver.firefox.geckodriver", ".C:\\Users\\ifeoma.emodi\\Desktop\\Automation Suite\\Firefox\\geckodriver.exe");
               driver = new FirefoxDriver();
       }
      // now we are going to check if the parameter is 'chrome'
       else if (browser.equalsIgnoreCase("chrome")){
           //set the path to chromedriver.exe but its different in mac os
           System.setProperty("webdriver.chrome.driver", ".C:\\Users\\ifeoma.emodi\\Desktop\\Automation Suite\\Chrome\\chromedriver.exe");
           //create chrome instance
           driver = new ChromeDriver();
       }
       // Check if the parametr passed is Edge or Internet Explorer
       else if(browser.equalsIgnoreCase("Edge")){
           //set pathe to the Edge.exe
           System.setProperty("webdriver.edge.driver", ".C:\\Users\\ifeoma.emodi\\Desktop\\Automation Suite\\Chrome\\chromedriver.exe");
           //create Edge instance
           driver = new EdgeDriver();
       }
    else{
           //if no browser passed throw exception
           throw new Exception("Browser is not setup correctly");
       }
       driver.manage().timeouts().implicitlyWait(10, TimeUnit.SECONDS);
   }

@After
   public void afterTest(){
       driver.quit();
}
}

